{
  "name": "multiagent-claude",
  "version": "2.0.0",
  "description": "Multi-agent orchestration framework for Claude Code with visual development support",
  "main": "cli/index.js",
  "bin": {
    "multiagent-claude": "./cli/index.js",
    "mac": "./cli/index.js"
  },
  "scripts": {
    "setup": "multiagent-claude init",
    "mcp:setup": "multiagent-claude mcp setup",
    "agents:parallel": "multiagent-claude parallel",
    "orchestrate": "multiagent-claude orchestrate",
    "verify": "multiagent-claude verify",
    "init": "node cli/index.js init",
    "init:memory": "node cli/index.js init --memory-only",
    "init:docs": "node cli/index.js init --with-docs",
    "memory:status": "node cli/index.js memory status",
    "memory:inspect": "node cli/index.js memory inspect",
    "agent:create": "node cli/index.js agent create",
    "agent:list": "node cli/index.js agent list",
    "command:create": "node cli/index.js command create",
    "command:list": "node cli/index.js command list",
    "test": "playwright test",
    "test:cli": "playwright test tests/cli-playwright.spec.js",
    "test:visual": "playwright test tests/visual-regression.spec.js",
    "test:agent-validation": "playwright test tests/agent-template-validation.spec.js",
    "test:quality": "npm run test:agent-validation && node scripts/quality-gates.js",
    "test:update-snapshots": "UPDATE_SNAPSHOTS=true playwright test tests/visual-regression.spec.js",
    "test:ci": "playwright test --reporter=blob",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "visual:test": "playwright test --config=playwright-visual.config.js",
    "visual:test:ui": "playwright test --config=playwright-visual.config.js --ui",
    "visual:test:debug": "playwright test --config=playwright-visual.config.js --debug",
    "visual:update": "playwright test --config=playwright-visual.config.js --update-snapshots",
    "visual:report": "playwright show-report .playwright/reports",
    "visual:setup": "node -e \"require('./cli/commands/mcp-setup.js').setupVisualDevelopment()\"",
    "visual:compare": "node -e \"require('./cli/utils/visual-compare.js').runComparison()\"",
    "visual:clean": "rm -rf .playwright/test-results .playwright/reports .claude/visual-iterations/*",
    "lint:staged": "echo 'Linting staged files...' && exit 0",
    "quality:check": "node scripts/quality-gates.js",
    "quality:dashboard": "node scripts/quality-dashboard.js",
    "agents:auto-fix": "node scripts/agent-auto-fix.js",
    "agents:auto-fix:dry-run": "node scripts/agent-auto-fix.js --dry-run",
    "agents:validate-fix": "npm run agents:auto-fix:dry-run && npm run test:quality"
  },
  "keywords": [
    "claude",
    "ai",
    "agent",
    "multi-agent",
    "orchestration",
    "memory",
    "claude-code",
    "development",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "express": "^5.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "inquirer": "^9.3.7",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@playwright/mcp": "^0.0.35",
    "@playwright/test": "^1.48.2",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "sharp": "^0.34.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/MultiAgent-Claude.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/MultiAgent-Claude/issues"
  },
  "homepage": "https://github.com/yourusername/MultiAgent-Claude#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "cli/",
    "Examples/",
    "templates/",
    "*.md",
    "LICENSE"
  ]
}
